<?xml version="1.0" encoding="UTF-8"?>

<phpunit bootstrap="test/bootstrap.php" colors="true">
    <testsuites>
        <testsuite name="alvtek-openid-connect-tests">
            <directory>./test/src</directory>
        </testsuite>
    </testsuites>
